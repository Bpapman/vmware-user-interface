class VisualizerCanvas extends Canvas 
{  
  
  
public void setup(PApplet p) 
  {
    println("Starting Network Visualizer.");
       
  }//end setup
  
public void draw(PApplet p) 
  {
    
    //Canvas Color and Size
    p.fill(255,255);
    p.rect(0,0,(screenWidth * 0.7),(screenHeight / 1.46));
    
  }//end draw
}// end Class