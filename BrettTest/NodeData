package jung1;

/**
 *
 * @author Brett
 */
public class NodeData {
  private String NodeName;
  private String Field1;
  private String Field2;
  private String Field3;

  public NodeData(String name, String f1, String f2, String f3)
  {
        NodeName = name;
        Field1 = f1;
        Field2 = f2;
        Field3 = f3;
  }

    public String getF1()
    {
        return Field1;
    }
    
    public void setF1(String f1)
    {
        this.Field1 = f1;
    }

    public void setName(String name)
    {
        this.NodeName = name;
    }

    public String getName()
    {
        return (NodeName);
    }

    public void setF2(String f2)
    {
        this.Field2 = f2;
    }

    public String getF2()
    {
        return (Field2);
    }

    public void setF3(String f3)
    {
        this.Field3 = f3;
    }
    
    public String getF3()
    {
        return (Field3);
    }
    
    public void nodePrint()
    {
        System.out.println("Name: " + this.getName());
        System.out.println(this.getF1());
        System.out.println(this.getF2());
        System.out.println(this.getF3());
        System.out.println();
    }
}
