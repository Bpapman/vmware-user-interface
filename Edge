package com.proj.vm;


public class Edge {
    
    //Edge Fields
    private String Orig;
    private String Dest;
    
    //Constructor
    public Edge(String EdgeOrig, String EdgeDest)
        {
            Orig = EdgeOrig;
            Dest = EdgeDest;
        }
    
    
    public void setOrig(String orig)
    {
        Orig = orig;
    }
    
    public void setDest(String dest)
    {
        Dest = dest;
    }
    
    public String getOrig()
    {
        return Orig;
    }
    
    public String getDest()
    {
        return Dest;
    }
    
    public void printEdge(Edge edge)
    {
        System.out.format("%s is connected to %s%n",edge.getOrig(),edge.getDest());
    }
    
    
}//end Edge class
